        "#funcion de fibonacci recursiva\n",
        "def fibonacci(n):\n",
        "\n",
        "  if n<=2 and n>=1:\n",
        "\n",
        "    return 1\n",
        "\n",
        "  else:\n",
        "\n",
        "    return fibonacci(n-1)+fibonacci(n-2)"
      ]
    }
  ]
